{
  "name": "resistance",
  "version": "0.0.1",
  "description": "Mobile app for playing \"The Resistance\" board game",
  "main": "index.js",
  "repository": "github.com/johnforster/resistance",
  "author": "John Forster",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node build/server.js",
    "build": "yarn build:client && yarn build:server",
    "build:client": "NODE_ENV=production webpack --config webpack.config.js",
    "build:server": "NODE_ENV=production babel src/server -d build --extensions .ts",
    "start:dev": "concurrently 'yarn start:dev:client' 'yarn start:dev:server'",
    "start:dev:server": "NODE_ENV=development ts-node-dev --respawn --transpileOnly src/server/server.ts",
    "start:dev:client": "NODE_ENV=development webpack --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.1",
    "@types/express-ws": "^3.0.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/webpack-env": "^1.14.0",
    "@types/websocket": "^0.0.40",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "websocket": "^1.0.29"
  }
}
