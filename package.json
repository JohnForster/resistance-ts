{
  "name": "resistance",
  "version": "0.0.1",
  "description": "Mobile app for playing \"The Resistance\" board game",
  "main": "build/server.js",
  "repository": "github.com/johnforster/resistance",
  "author": "John Forster",
  "license": "MIT",
  "workspaces": [
    "src/*"
  ],
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node src/server/build/server.js",
